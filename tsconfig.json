{
  "compilerOptions": {
    "target": "ES2022",
  "module": "NodeNext",
  "moduleResolution": "NodeNext",
    "outDir": "./build",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    
  "declaration": false,
  "emitDeclarationOnly": false,
  "noEmit": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build"]
}
